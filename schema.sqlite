CREATE TABLE access (
accessid INTEGER PRIMARY KEY,
nick VARCHAR(64) NOT NULL,
privs VARCHAR(255) NULL
);
CREATE TABLE bad_responses (
responseid INTEGER PRIMARY KEY,
response VARCHAR(255) NOT NULL
);
CREATE TABLE channels (
channelid INTEGER PRIMARY KEY,
network VARCHAR(30) NOT NULL,
name VARCHAR(64) NOT NULL,
onjoincmd VARCHAR(255) NULL,
modules VARCHAR(255) NULL
);
CREATE TABLE dislike_responses (
responseid INTEGER PRIMARY KEY,
response VARCHAR(255) NOT NULL
);
CREATE TABLE dislikes (
dislikeid INTEGER PRIMARY KEY,
dislike VARCHAR(64) NOT NULL
);
CREATE TABLE eightball_responses (
responseid INTEGER PRIMARY KEY,
response VARCHAR(255) NOT NULL
);
CREATE TABLE good_responses (
responseid INTEGER PRIMARY KEY,
response VARCHAR(255) NOT NULL
);
CREATE TABLE ignore_nicks (
ignoreid INTEGER PRIMARY KEY,
nick VARCHAR(64) NOT NULL
);
CREATE TABLE misc (
miscid INTEGER PRIMARY KEY,
name VARCHAR(255) NOT NULL,
value VARCHAR(255) NULL
);
CREATE TABLE modules (
moduleid INTEGER PRIMARY KEY,
name VARCHAR(64) NOT NULL
);
CREATE TABLE networks (name VARCHAR(30) PRIMARY KEY, host VARCHAR(60) NOT NULL, port INT(7) NOT NULL, nick VARCHAR(20) NOT NULL, identcmd VARCHAR(255) NOT NULL, username VARCHAR(30) NOT NULL, ircname VARCHAR(255) NOT NULL);
CREATE TABLE random_responses (
responseid INTEGER PRIMARY KEY,
response VARCHAR(255) NOT NULL
);
CREATE TABLE weather_aliases (
aliasid INTEGER PRIMARY KEY,
network VARCHAR(30) NOT NULL,
nick VARCHAR(20) NOT NULL,
alias VARCHAR(20) NULL,
realplace VARCHAR(40) NOT NULL
);
